+	$result = $mysqli->query("SELECT * FROM ep_term_taxonomy AS tt INNER JOIN ep_terms AS t ON tt.term_id = t.term_id WHERE taxonomy LIKE 'product_cat' AND parent = ".$parent);
+	$output = '';
+	if($result->num_rows){
