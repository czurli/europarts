+}
+
+@mixin back-pulse {
+	@include hacks();
+	overflow: hidden;
+	@include prefixed(transition-duration, $slowDuration);
+	@include prefixed(transition-property, "color, background-color");
+
+	&:hover,
+	&:focus,
