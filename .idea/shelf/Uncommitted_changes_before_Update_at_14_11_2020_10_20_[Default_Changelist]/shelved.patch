+    })
+    $('.woocommerce-product-gallery__wrapper').append($('.onsale'));
+
+    // aggiungo effetti css ai bottoni
+    $('.woocommerce .button').addClass('hvr-underline-from-left');
+    $('.woocommerce .edit').addClass('hvr-underline-from-left');
+
+    // barra di ricerca //
+    if ($(window).width() < 991) {
