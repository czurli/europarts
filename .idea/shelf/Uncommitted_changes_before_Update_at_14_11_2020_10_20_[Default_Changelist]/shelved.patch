+            add_to_cart_button.attr("data-quantity", val_add_to_cart_button - 1);
+            add_to_cart_button.attr("href", "?add-to-cart=" + add_to_cart_button.attr("data-product_id") + "&quantity=" + (val_add_to_cart_button - 1));
+        }
+    });
